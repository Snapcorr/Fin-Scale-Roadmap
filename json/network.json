{
  "network": {
    "alpaca": {
      "base_url": "https://paper-api.alpaca.markets",
      "data_url": "https://data.alpaca.markets",
      "endpoints": {
        "account": "/v2/account",
        "orders": "/v2/orders",
        "positions": "/v2/positions",
        "assets": "/v2/assets",
        "portfolio": "/v2/account/portfolio/history"
      }
    },
    "fmp": {
      "base_url": "https://financialmodelingprep.com/api/v3",
      "endpoints": {
        "profile": "/profile/{symbol}",
        "ratios": "/ratios/{symbol}",
        "historical": "/historical-price-full/{symbol}",
        "screener": "/stock-screener"
      }
    },
    "rate_limits": {
      "alpaca": {
        "requests_per_min": 200,
        "burst_limit": 50
      },
      "fmp": {
        "requests_per_min": 300,
        "burst_limit": 60
      }
    },
    "timeouts": {
      "default": 5000,
      "api_call": 7000
    }
  }
}
